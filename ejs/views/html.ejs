<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <h1>
        Guia perguntas!!
    </h1>
    <h2>rotina de Programador</h2>
    <div class=container>
        <div class="box">

            <li>
                <!-- //desta forma voce consegue acessar a variavel -->
                <%= nome %>
            </li>
            <li>
                <%= lang %>
            </li>
            <li>
                <%= empresa %>
            </li>
            <li>
                <%= inscritos %>
            </li>
            <li>
                <%= msgParam %>
            </li>

            <% if(msg===true && msgParam==='antonio' ){ %>
                <p>Você e forte</p>
                <%}else{%>
                    <p>Você não e forte</p>
                    <%}%>

        </div>

             <!--Usando estrtura de repeticao   -->
        <div class="box-size">

            <% produtos.forEach(function(produto){%>
                <hr>
                <p><%= produto.nome %></p>
                <hr>
                <p><%= produto.preco %> </p>

                <% }) %>
        </div>

    </div>
</body>

</html>